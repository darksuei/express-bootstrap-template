{
  "name": "null",
  "version": "0.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development nodemon",
    "lint": "eslint . --ext .ts",
    "clean": "rimraf dist",
    "build": "esbuild src/index.ts --bundle --platform=node --target=node18 --outfile=dist/index.js --minify",
    "start": "NODE_ENV=production node dist/index.js",
    "typeorm": "npx typeorm-ts-node-commonjs --dataSource ./src/database/dataSource.ts",
    "typeorm:entity:create": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm entity:create ./src/entity",
    "typeorm:migrate": "npm run typeorm -- migration:run",
    "typeorm:generate": "npm run typeorm -- migration:generate ./src/database/migrations/migration",
    "typeorm:revert": "npm run typeorm -- migration:revert",
    "test": "NODE_ENV=test jest"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "http-status": "1.7.4",
    "morgan": "^1.10.0",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.2.2",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "typescript": "^5.4.4",
    "typescript-eslint": "^8.31.1",
    "winston": "^3.13.0",
    "yamljs": "^0.3.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-useragent": "^1.0.5",
    "@types/morgan": "^1.9.9",
    "@types/swagger-ui-express": "^4.1.7",
    "@types/yamljs": "^0.2.34",
    "esbuild": "^0.25.3",
    "esbuild-register": "^3.6.0",
    "eslint": "^9.25.1",
    "nodemon": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2"
  }
}
